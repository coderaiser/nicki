{
  "name": "nicki",
  "version": "2.0.1",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "Get names of users by uids from /etc/passwd",
  "homepage": "http://github.com/coderaiser/nicki",
  "main": "lib/nicki.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/nicki.git"
  },
  "scripts": {
    "lint": "eslint lib",
    "6to5": "buble lib -o legacy",
    "build": "redrun 6to5 legacy",
    "legacy": "echo \"module.exports = require('./nicki');\" > legacy/index.js",
    "wisdom": "npm run build",
    "test": "tape test/*.js",
    "watcher": "nodemon -w test -w lib --exec",
    "watch:test": "npm run watcher -- npm test",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "uid",
    "name",
    "passwd"
  ],
  "dependencies": {
    "ischanged": "^1.0.2",
    "spawnify": "^3.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "buble": "^0.14.2",
    "coveralls": "^2.11.15",
    "eslint": "^3.10.2",
    "nyc": "^9.0.1",
    "tape": "^4.6.2"
  }
}
